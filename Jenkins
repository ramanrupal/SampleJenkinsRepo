node {
   stage('Checkout') {
        checkout([
            $class: 'GitSCM',
            branches: scm.branches,
            extensions: scm.extensions + [[$class: 'LocalBranch'], [$class: 'WipeWorkspace']],
            userRemoteConfigs: [[url: 'http://gsgit.gslab.com/dipti_bagal/jenkins-k8s.git']],
            doGenerateSubmoduleConfigurations: false
        ])
        
   stage('git branch'){
   sh "git chekout prod"
   sh "ls -al"
   }
}
